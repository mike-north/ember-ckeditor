{
	"name": "ember-ckeditor5",
	"version": "0.1.3",
	"description": "CKEditor 5 (Balloon build) for Ember apps",
	"keywords": [
		"ember-addon"
	],
	"repository": "https://github.com/mike-north/ember-ckeditor",
	"license": "MIT",
	"author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
	"directories": {
		"doc": "doc",
		"test": "tests"
	},
	"scripts": {
		"build": "ember build",
		"deploy": "GH_PAGES=true ember build && GH_PAGES=true ember github-pages:commit --environment development --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
		"lint:hbs": "ember-template-lint .",
		"lint:js": "eslint .",
		"start": "ember serve",
		"test": "ember test",
		"test:all": "ember try:each"
	},
	"dependencies": {
		"@ckeditor/ckeditor5-build-balloon": "1.0.0-beta.4",
		"ember-cli-babel": "6.17.2",
		"ember-cli-htmlbars": "3.0.1"
	},
	"devDependencies": {
		"@ember/optional-features": "0.6.4",
		"broccoli-asset-rev": "2.7.0",
		"ember-ajax": "3.1.0",
		"ember-cli": "3.5.0",
		"ember-cli-dependency-checker": "3.0.0",
		"ember-cli-eslint": "4.2.3",
		"ember-cli-github-pages": "0.2.0",
		"ember-cli-htmlbars-inline-precompile": "2.0.0",
		"ember-cli-inject-live-reload": "1.10.2",
		"ember-cli-release": "1.0.0-beta.2",
		"ember-cli-sri": "2.1.1",
		"ember-cli-template-lint": "1.0.0-beta.2",
		"ember-cli-uglify": "2.1.0",
		"ember-disable-prototype-extensions": "1.1.3",
		"ember-export-application-global": "2.0.0",
		"ember-load-initializers": "1.1.0",
		"ember-maybe-import-regenerator": "0.1.6",
		"ember-qunit": "3.5.3",
		"ember-resolver": "5.0.1",
		"ember-source": "3.5.1",
		"ember-source-channel-url": "1.1.0",
		"ember-test-helpers": "0.6.3",
		"ember-try": "1.1.0",
		"eslint-plugin-ember": "5.2.0",
		"eslint-plugin-node": "7.0.1",
		"loader.js": "4.7.0",
		"qunit-dom": "0.8.0"
	},
	"engines": {
		"node": "6.* || 8.* || >= 10.*"
	},
	"ember-addon": {
		"configPath": "tests/dummy/config"
	}
}
