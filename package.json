{
  "name": "ember-ckeditor5",
  "version": "0.1.3",
  "description": "CKEditor 5 (Balloon build) for Ember apps",
  "keywords": [
    "ember-addon"
  ],
  "license": "MIT",
  "author": "Mike North <michael.l.north@gmail.com> (https://mike.works)",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/mike-north/ember-ckeditor",
  "scripts": {
    "build": "ember build",
    "deploy": "GH_PAGES=true ember build && GH_PAGES=true ember github-pages:commit --environment development --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\" && git push origin gh-pages:gh-pages",
    "lint:js": "eslint ./*.js addon addon-test-support app config lib server test-support tests",
    "start": "ember serve",
    "test": "ember try:each"
  },
  "dependencies": {
    "@ckeditor/ckeditor5-build-balloon": "^1.0.0-alpha.2",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-htmlbars": "^2.0.1"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~2.18.0",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-github-pages": "^0.1.3",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-release": "^1.0.0-beta.2",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-disable-prototype-extensions": "^1.1.2",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-resolver": "^5.0.0",
    "ember-source": "~2.18.0",
    "ember-test-helpers": "~0.6.0",
    "eslint-plugin-ember": "^5.0.0",
    "eslint-plugin-node": "^5.2.1",
    "loader.js": "^4.2.3"
  },
  "engines": {
    "node": "^4.5 || 6.* || >= 7.*"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config"
  }
}
